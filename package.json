{
  "name": "ct553-server",
  "module": "index.js",
  "type": "module",
  "scripts": {
    "start": "sudo docker compose up -d && bun --watch server.js",
    "db": "bun x prisma migrate dev --name update",
    "db_backup": "pg_dump -U dhoaibao -h localhost -p 5432 toystore > backup.sql && git add backup.sql && git commit -m 'backup database'",
    "db_restore": "psql -U dhoaibao -h localhost -p 5432 -d toystore -f backup.sql"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@supabase/supabase-js": "^2.49.1",
    "base64-arraybuffer": "^1.0.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "ct553-server": ".",
    "express": "^4.21.2",
    "firebase": "^11.4.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "pg": "^8.14.1",
    "prisma": "^6.5.0",
    "redis": "^4.7.0",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1"
  }
}